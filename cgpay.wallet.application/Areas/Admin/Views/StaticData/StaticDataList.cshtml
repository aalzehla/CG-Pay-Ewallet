<script>
    $('.icon-lock2').parent().on('click', function e() {
        /*debugger*/;
        let sdatatypeId = $(this).attr("data-val1");
        let sdataid = $(this).attr("data-val");
        let tId = $(this).closest("tr").attr("id");
        jsonData = "{" +
            "__RequestVerificationToken: '" + $('input[name="__RequestVerificationToken"]').val() + "'," +
             "sdatatypeId:'" + sdatatypeId + "',sdataid:'"+sdataid+"'}";
        var funcText = "CallAjaxDbResponse(" + jsonData + ",'/Admin/StaticData/DisableSData','POST',false,'window.location.reload();')";
        showConfirmationModal("Are you sure you want to Disable this Category?", funcText);
    })
    $('.icon-unlocked2').parent().on('click', function e() {
       let sdatatypeId = $(this).attr("data-val1");
        let sdataid = $(this).attr("data-val");
        jsonData = "{" +
            "__RequestVerificationToken: '" + $('input[name="__RequestVerificationToken"]').val() + "'," +
            "sdatatypeId:'" + sdatatypeId + "',sdataid:'"+sdataid+"'}";
        var funcText = "CallAjaxDbResponse(" + jsonData + ",'/Admin/StaticData/EnableSData','POST',false,'window.location.reload();')";
        showConfirmationModal("Are you sure you want to Enable this Category?", funcText);
    })
</script>